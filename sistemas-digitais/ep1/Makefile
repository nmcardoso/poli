async:
	ghdl -a mmc_async.vhd
	ghdl -a mmc_tb.vhd
	ghdl -r mmc_tb --vcd=mmc.vcd

sync:
	ghdl -a mmc_sync.vhd
	ghdl -a mmc_tb.vhd
	ghdl -r mmc_tb --vcd=mmc.vcd

